{
  "expo": {
    "name": "FlipBoard",
    "slug": "FlipBoard",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flipboardLogo.png",
    "splash": {
      "image": "./assets/flipboard_splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6F6F6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
